{
  "name": "react-default-file-content",
  "publisher": "tilio",
  "displayName": "React default file content",
  "description": "Sets up a function component in newly created tsx files",
  "repository": "https://github.com/TilioApp/react-default-file-content.git",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.37.0",
    "glob": "^7.1.4",
    "typescript": "^3.3.1"
  }
}
